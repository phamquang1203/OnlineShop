        <servlet-name>AddUsersController</servlet-name>
        <url-pattern>/addUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProductController</servlet-name>
        <url-pattern>/addProduct</url-pattern>
    </servlet-mapping>
